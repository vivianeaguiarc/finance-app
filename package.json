{
    "name": "financeapp",
    "version": "1.0.0",
    "description": "Aplicação de gestão de finanças ",
    "main": "index.js",
    "type": "module",
    "scripts": {
       "test": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
        "test:ci": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --passWithNoTests --coverage",
        "coverage": "dotenv -e .env.test -- node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --passWithNoTests --coverage",
        "prepare": "husky install",
        "prisma:generate": "prisma generate",
        "migrations": "node src/db/postgres/migrations/exec.js",
        "dev": "node --watch index.js",
        "start": "node index.js",
        "eslint:check": "eslint src --ext .js",
        "prettier:check": "prettier --check src"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/vivianeaguiarc/finance-app.git"
    },
    "author": "Viviane Silva",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/vivianeaguiarc/finance-app/issues"
    },
    "homepage": "https://github.com/vivianeaguiarc/finance-app#readme",
    "devDependencies": {
        "@babel/preset-env": "^7.28.5",
        "@commitlint/cli": "^20.1.0",
        "@commitlint/config-conventional": "^20.0.0",
        "@eslint/js": "^9.39.1",
        "@faker-js/faker": "^10.1.0",
        "@types/jest": "^30.0.0",
        "babel-jest": "^30.2.0",
        "dotenv-cli": "^11.0.0",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "git-commit-msg-linter": "^5.0.8",
        "husky": "^9.1.7",
        "jest": "^30.2.0",
        "lint-staged": "^16.2.6",
        "prettier": "^3.6.2",
        "prisma": "^6.19.0",
        "supertest": "^7.1.4"
    },
    "dependencies": {
        "@prisma/client": "^6.19.0",
        "bcrypt": "^6.0.0",
        "bcryptjs": "^3.0.3",
        "chalk": "^5.6.2",
        "dayjs": "^1.11.19",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.16.3",
        "swagger-ui-express": "^5.0.1",
        "uuid": "^13.0.0",
        "validator": "^13.15.23",
        "zod": "^4.1.12"
    }
